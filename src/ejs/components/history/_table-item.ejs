<tr class="text-sm hover:bg-gray-50 dark:hover:bg-darkGray-700">
  <td class="px-3 sm:px-6 py-3 sm:py-4 whitespace-nowrap">
    <div class="flex items-center">
      <% if (type==='receive' ) { %>
        <div class="p-3 bg-blue-50 dark:bg-darkGray-700 rounded-full mr-3">
          <svg class="w-6 h-6 text-green-500 dark:text-green-400" xmlns="http://www.w3.org/2000/svg" fill="none"
            viewBox="0 0 24 24" stroke="currentColor">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 14l-7 7m0 0l-7-7m7 7V3" />
          </svg>
        </div>
        <div class="font-semibold text-blue-900 dark:text-darkGray-100">Receive</div>

        <% } else if (type==='send' ) { %>
          <div class="p-3 bg-blue-50 dark:bg-darkGray-700 rounded-full mr-3">
            <svg class="w-6 h-6 text-red-500 dark:text-red-400" xmlns="http://www.w3.org/2000/svg" fill="none"
              viewBox="0 0 24 24" stroke="currentColor">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 10l7-7m0 0l7 7m-7-7v18" />
            </svg>
          </div>
          <div class="font-semibold text-blue-900 dark:text-darkGray-100">Send</div>

          <% } else if (type==='trade' ) { %>
            <div class="p-3 bg-blue-50 dark:bg-darkGray-700 rounded-full mr-3">
              <svg class="w-6 h-6 text-blue-500 dark:text-blue-400" xmlns="http://www.w3.org/2000/svg" fill="none"
                viewBox="0 0 24 24" stroke="currentColor">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                  d="M8 7h12m0 0l-4-4m4 4l-4 4m0 6H4m0 0l4 4m-4-4l4-4" />
              </svg>
            </div>
            <div class="font-semibold text-blue-900 dark:text-darkGray-100">Trade</div>

            <% } else if (type==='claim' ) { %>
              <div class="p-3 bg-blue-50 dark:bg-darkGray-700 rounded-full mr-3">
                <svg class="w-6 h-6 text-purple-500 dark:text-purple-400" xmlns="http://www.w3.org/2000/svg" fill="none"
                  viewBox="0 0 24 24" stroke="currentColor">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                    d="M9 5H7a2 2 0 00-2 2v12a2 2 0 002 2h10a2 2 0 002-2V7a2 2 0 00-2-2h-2M9 5a2 2 0 002 2h2a2 2 0 002-2M9 5a2 2 0 012-2h2a2 2 0 012 2m-6 9l2 2 4-4" />
                </svg>
              </div>
              <div class="font-semibold text-blue-900 dark:text-darkGray-100">Claim</div>

              <% } else if (type==='mint' ) { %>
                <div class="p-3 bg-blue-50 dark:bg-darkGray-700 rounded-full mr-3">
                  <svg class="w-6 h-6 text-indigo-500 dark:text-indigo-400" xmlns="http://www.w3.org/2000/svg"
                    fill="none" viewBox="0 0 24 24" stroke="currentColor">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                      d="M12 8c-1.657 0-3 .895-3 2s1.343 2 3 2 3 .895 3 2-1.343 2-3 2m0-8c1.11 0 2.08.402 2.599 1M12 8V7m0 1v8m0 0v1m0-1c-1.11 0-2.08-.402-2.599-1M21 12a9 9 0 11-18 0 9 9 0 0118 0z" />
                  </svg>
                </div>
                <div class="font-semibold text-blue-900 dark:text-darkGray-100">Mint</div>

                <% } else if (type==='burn' ) { %>
                  <div class="p-3 bg-blue-50 dark:bg-darkGray-700 rounded-full mr-3">
                    <svg class="w-6 h-6 text-yellow-500 dark:text-yellow-400" xmlns="http://www.w3.org/2000/svg"
                      fill="none" viewBox="0 0 24 24" stroke="currentColor">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                        d="M17.657 18.657A8 8 0 016.343 7.343S7 9 9 10c0-2 .5-5 2.986-7C14 5 16.09 5.777 17.656 7.343A7.975 7.975 0 0120 13a7.975 7.975 0 01-2.343 5.657z" />
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                        d="M9.879 16.121A3 3 0 1012.015 11L11 14H9c0 .768.293 1.536.879 2.121z" />
                    </svg>
                  </div>
                  <div class="font-semibold text-blue-900 dark:text-darkGray-100">Burn</div>

                  <% } else if (type==='approve' ) { %>
                    <div class="p-3 bg-blue-50 dark:bg-darkGray-700 rounded-full mr-3">
                      <svg class="w-6 h-6 text-pink-500 dark:text-pink-400" xmlns="http://www.w3.org/2000/svg"
                        fill="none" viewBox="0 0 24 24" stroke="currentColor">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
                      </svg>
                    </div>
                    <div class="font-semibold text-blue-900 dark:text-darkGray-100">Approve</div>

                    <% } else if (type==='execute' ) { %>
                      <div class="p-3 bg-blue-50 dark:bg-darkGray-700 rounded-full mr-3">
                        <svg class="w-6 h-6 text-gray-600 dark:text-darkGray-300" xmlns="http://www.w3.org/2000/svg"
                          fill="none" viewBox="0 0 24 24" stroke="currentColor">
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                            d="M10.325 4.317c.426-1.756 2.924-1.756 3.35 0a1.724 1.724 0 002.573 1.066c1.543-.94 3.31.826 2.37 2.37a1.724 1.724 0 001.065 2.572c1.756.426 1.756 2.924 0 3.35a1.724 1.724 0 00-1.066 2.573c.94 1.543-.826 3.31-2.37 2.37a1.724 1.724 0 00-2.572 1.065c-.426 1.756-2.924 1.756-3.35 0a1.724 1.724 0 00-2.573-1.066c-1.543.94-3.31-.826-2.37-2.37a1.724 1.724 0 00-1.065-2.572c-1.756-.426-1.756-2.924 0-3.35a1.724 1.724 0 001.066-2.573c-.94-1.543.826-3.31 2.37-2.37.996.608 2.296.07 2.572-1.065z" />
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                            d="M15 12a3 3 0 11-6 0 3 3 0 016 0z" />
                        </svg>
                      </div>
                      <div class="font-semibold text-blue-900 dark:text-darkGray-100">Execute</div>

                      <% } %>
    </div>

  </td>
  <td class="text-gray-500 dark:text-darkGray-400 whitespace-nowrap">02:13 PM</td>
  <td class="px-6 py-4 whitespace-nowrap w-full">
    <% if (type==='receive' || type==='claim' ||type==='mint' ) { %>
      <div class="flex items-center">
        <div class="h-9 w-9 rounded-full overflow-hidden mr-2">
          <img src="images/ACA.96bf6b59.svg" class="h-full w-full">
        </div>
        <div>
          <p class="text-green-500 dark:text-green-400 font-medium leading-tight">+20PLM</p>
          <p class="text-blue-900 dark:text-darkGray-100">$2.19</p>
        </div>
      </div>
      <% } else if (type==='send' ||type==='burn' ) { %>
        <div class="flex items-center">
          <div class="h-9 w-9 rounded-full overflow-hidden mr-2">
            <img src="images/ACA.96bf6b59.svg" class="h-full w-full">
          </div>
          <div>
            <p class="text-red-500 dark:text-red-400 font-medium leading-tight">-20PLM</p>
            <p class="text-blue-900 dark:text-darkGray-100">$2.19</p>
          </div>
        </div>

        <% } else if (type==='trade' ) { %>
          <div class="flex items-center">
            <div class="flex items-center">
              <div class="h-9 w-9 rounded-full overflow-hidden mr-2">
                <img src="images/ACA.96bf6b59.svg" class="h-full w-full">
              </div>
              <div>
                <p class="text-green-500 dark:text-green-400 font-medium leading-tight">+20PLM</p>
                <p class="text-blue-900 dark:text-darkGray-100">$2.19</p>
              </div>
            </div>
            <div>
              <svg class="h-6 w-6 text-gray-500 dark:text-darkGray-400 mx-2" xmlns="http://www.w3.org/2000/svg"
                fill="none" viewBox="0 0 24 24" stroke="currentColor">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M14 5l7 7m0 0l-7 7m7-7H3" />
              </svg>
            </div>
            <div class="flex items-center">
              <div class="h-9 w-9 rounded-full overflow-hidden mr-2">
                <img src="images/ACA.96bf6b59.svg" class="h-full w-full">
              </div>
              <div>
                <p class="text-red-500 dark:text-red-400 font-medium leading-tight">-20PLM</p>
                <p class="text-blue-900 dark:text-darkGray-100">$2.19</p>
              </div>
            </div>
          </div>

          <% } %>
  </td>

<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="format-detection" content="telephone=no">
  <link href="css/style.css" rel="stylesheet">
  <title>Plasm App UI</title>
  <meta name="description" content="">
</head>

<body>
  <div class="h-screen flex overflow-hidden bg-gray-50 dark:bg-darkGray-900">

    <%- include('./components/common/_sidebar-mobile'); -%>

      <%- include('./components/common/_sidebar-desktop'); -%>



        <div class="flex flex-col w-0 flex-1 overflow-y-auto lg:overflow-hidden">

          <%- include('./components/common/_header-mobile'); -%>

            <main class="flex-1 relative z-0 lg:overflow-y-auto overflow-x-hidden focus:outline-none">


              <div class="px-4 py-10 md:px-8">

                <div class="flex justify-between">
                  <h1 class="text-2xl font-extrabold text-blue-900 dark:text-white mb-2">Staking</h1>
                  <%- include('./components/common/_address-change'); -%>
                </div>

                <%- include('./components/staking/_tab'); -%>

                  <%- include('./components/staking/_stash-controller', {btn:'Stake'}); -%>

                    <%- include('./components/staking/_status-example', {btn:'Stake'}); -%>

                      <%- include('./components/staking/_search-form'); -%>

                        <div class="flex flex-col mb-8">
                          <div class="-my-2 overflow-x-auto sm:-mx-6 lg:-mx-8">
                            <div class="py-2 align-middle inline-block min-w-full sm:px-6 lg:px-8">
                              <div
                                class="shadow overflow-hidden border-b border-gray-200 dark:border-darkGray-600 rounded-lg">
                                <table class="min-w-full divide-y divide-gray-200 dark:divide-darkGray-600">
                                  <thead class="bg-white dark:bg-darkGray-800">
                                    <tr>
                                      <th scope="col"
                                        class="px-3 py-2 sm:px-6 sm:py-4 text-left text-xs font-medium text-gray-500 dark:text-darkGray-400 uppercase tracking-wider text-center">
                                        Stake
                                      </th>
                                      <th scope="col"
                                        class="px-3 py-2 sm:px-6 sm:py-4 text-left text-xs font-medium text-gray-500 dark:text-darkGray-400 uppercase tracking-wider">
                                        Status
                                      </th>
                                      <th scope="col"
                                        class="px-3 py-2 sm:px-6 sm:py-4 text-left text-xs font-medium text-gray-500 dark:text-darkGray-400 uppercase tracking-wider">
                                        Name
                                      </th>
                                      <th scope="col"
                                        class="px-3 py-2 sm:px-6 sm:py-4 text-left text-xs font-medium text-gray-500 dark:text-darkGray-400 uppercase tracking-wider">
                                        Users
                                      </th>
                                      <th scope="col"
                                        class="px-3 py-2 sm:px-6 sm:py-4 text-left text-xs font-medium text-gray-500 dark:text-darkGray-400 uppercase tracking-wider">
                                        Staked Value
                                      </th>
                                      <th scope="col" class="px-3 py-2 sm:px-6 sm:py-4">
                                        <span class="sr-only">Link</span>
                                      </th>
                                    </tr>
                                  </thead>
                                  <tbody
                                    class="bg-white dark:bg-darkGray-800 divide-y divide-gray-200 dark:divide-darkGray-600">
                                    <%- include('./components/staking/_table-item', {type:'stake'}); -%>
                                      <%- include('./components/staking/_table-item', {type:'stake'}); -%>
                                        <%- include('./components/staking/_table-item', {type:'stake'}); -%>
                                          <%- include('./components/staking/_table-item', {type:'stake'}); -%>
                                            <%- include('./components/staking/_table-item', {type:'stake'}); -%>
                                              <%- include('./components/staking/_table-item', {type:'stake'}); -%>
                                  </tbody>
                                </table>
                              </div>
                            </div>
                          </div>
                        </div>

              </div>

            </main>
        </div>
  </div>
</body>

</html>
